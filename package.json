{
  "name": "test-angular2",
  "private": true,
  "scripts": {
    "start": "meteor run --port 3005",
    "start-profiling": "meteor run --port 3009 --extra-packages bundle-visualizer --production",
    "postinstall": "npm run fonts:install",
    "prefonts:install": "mkdirp ./client/stylesheets",
    "fonts:install": "npm run ui-grid-fonts:install & npm run raleway-fonts:install & npm run lato-fonts:install & npm run font-awesome:install",
    "font-awesome:install": "cpy \"node_modules/font-awesome/fonts/fontawesome-webfont.{woff,woff2,eot,svg,ttf}\" public/fonts/font-awesome",
    "ui-grid-fonts:install": "cpy \"node_modules/angular-ui-grid/ui-grid.{woff,eot,svg,ttf}\" public/fonts/angular-ui-grid",
    "raleway-fonts:install": "goog-webfont-dl -a -d ./public/fonts/raleway -p '../fonts/raleway' -o ./client/stylesheets/raleway-fonts.css -y 400,300,600,700,900 Raleway",
    "lato-fonts:install": "goog-webfont-dl -a -d ./public/fonts/lato -p '../fonts/lato' -o ./client/stylesheets/lato-fonts.css -y 400,700,900,300 Lato"
  },
  "dependencies": {
    "@angular/animations": "^4.3.3",
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/forms": "^4.3.3",
    "@angular/http": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "@angular/platform-server": "^4.3.3",
    "@angular/router": "^4.3.3",
    "@angular/upgrade": "^4.3.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "angular": "^1.6.6",
    "angular-ui-grid": "^4.0.6",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.0.0-beta",
    "c3": "^0.4.18",
    "d3": "^4.10.2",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "meteor-node-stubs": "^0.2.11",
    "meteor-rxjs": "^0.4.8",
    "moment": "^2.18.1",
    "ngx-webstorage": "^1.8.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "web-animations-js": "^2.3.1",
    "xlsx": "^0.11.4",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.3",
    "@angular/language-service": "^4.4.3",
    "cpy-cli": "^1.0.1",
    "goog-webfont-dl": "^1.0.1",
    "mkdirp": "^0.5.1"
  }
}
